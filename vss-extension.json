{
    "manifestVersion": 1,
    "id": "git-buildtasks",
    "name": "git tasks",
    "version": "0.0.1",
    "publisher": "sander-aernouts",
    "public" : false,
    "repository": {
        "type": "git",
        "uri": "https://github.com/sanderaernouts/vsts-git-tasks"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Git related tasks for VSTS",
    "categories": [
        "Build and release"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "tags": [
    ],
    "links": {
        "home": {
            "uri": "https://github.com/sanderaernouts/vsts-git-tasks"
        },
        "repository": {
            "uri": "https://github.com/sanderaernouts/vsts-git-tasks"
        },
        "issues": {
            "uri": "https://github.com/sanderaernouts/vsts-git-tasks/issues"
        }
    },
    "branding": {
        "color": "rgb(248,153,56)",
        "theme": "light"
    },
    "icons": {
        "default": "extension-icon.png"        
    },
    "files": [
        {
            "path": "createPullRequest/createPullRequest.js"
          },
          {
            "path": "createPullRequest/node_modules"
          },
          {
            "path": "createPullRequest/task.json"
          },
          {
            "path": "createPullRequest/icon.png"
      }
    ],
    "contributions": [
        {
            "id": "git-create-pullrequest",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "createpullrequest"
            }
        }
    ]
}