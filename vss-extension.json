{
    "manifestVersion": 1,
    "id": "Create-Pull-Request",
    "name": "Create Pull Request",
    "description": "Build task to automatically create pull requests",
    "helpMarkDown": "Forked from https://github.com/sanderaernouts/vsts-git-tasks.git. All credits to original author(s).",
    "version": "1.0.0",
    "publisher": "ArpitKhandelwal",
    "public" : false,
    "repository": {
        "type": "git",
        "uri": "https://github.com/ArpitLabs/vsts-git-tasks.git"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    
    "categories": [
        "Azure Pipelines"
    ],
    "screenshots": [
        {
          "path": "images/screenshots/createpullrequest.png"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "license.md"
        }
    },
    "tags": [
        "git",
        "build",
        "pullrequest",
        "Xpirit"
    ],
    "links": {
        "home": {
            "uri": "https://github.com/ArpitLabs/vsts-git-tasks"
        },
        "repository": {
            "uri": "https://github.com/ArpitLabs/vsts-git-tasks"
        },
        "issues": {
            "uri": "https://github.com/ArpitLabs/vsts-git-tasks/issues"
        }
    },
    "branding": {
        "color": "rgb(248,153,56)",
        "theme": "light"
    },
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "createpullrequest/createPullRequest.ts"
          },
          {
            "path": "createpullrequest/node_modules"
          },
          {
            "path": "createpullrequest/task.json"
          },
          {
            "path": "createpullrequest/icon.png"
      }
    ],
    "contributions": [
        {
            "id": "create-pull-requests-azure-devops",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "createpullrequest"
            }
        }
    ]
}